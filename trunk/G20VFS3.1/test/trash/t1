createvfs demo 1000
makedir /a/b/c/d e
makedir /a/b/f/g h
makedir /p/o/i/u h
movedir /a/b/f /a/b/f/g
movedir /a/b/f /a/b/c
listdir / 1 outputdir
unmount demo1
unmount demo
mount demo
addfile /a/b/c/f/g abc1.txt abc.txt
listdir / 1 outputdir
copyfile a/b/c/f/g/abc1.txt /
exportfile /abc1.txt abc2.txt
movefile /abc1.txt /p/o/i/u
listdir / 1 outputdir
unmount


