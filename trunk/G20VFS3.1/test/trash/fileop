createvfs demo 9024
mountvfs demo 
makedir /home d
makedir /home a
addfile /home/d demo.txt abc.txt
listdir /home 1 outputdir 


movefile /home/d/demo.txt /home/a

listdir /home 1 outputdir 

copyfile /home/a/demo.txt /home
listdir /home 1 outputdir 
removefile /home/a/demo.txt
listdir /home 1 outputdir 
searchfile demo.txt outputdir

unmountvfs demo
 
